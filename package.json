{
  "name": "fc-sync",
  "version": "0.0.12",
  "description": "This is a component demo for Serverless Devs Tool ",
  "keywords": [
    "Serverless",
    "Framework",
    "Serverless-Devs",
    "Serverless-Tool",
    "Function Compute"
  ],
  "homepage": "https://www.serverless-devs.com",
  "author": "Serverless-Devs",
  "contributors": [],
  "autoInstall": false,
  "license": "MIT",
  "main": "./dist/index.js",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ServerlessTool/express-alibaba"
  },
  "scripts": {
    "start": "npm run watch",
    "watch": "tsc -w",
    "prebuild": "rm -rf node_modules && rm -rf package-lock.json && npm i && rm -rf dist",
    "build": "npm run esbuild",
    "esbuild": "esbuild src/index.ts --bundle --log-level=error --minify --platform=node --format=cjs --target=node10.4 --external:@serverless-devs/core --outfile=dist/index.js",
    "test": "jest --testNamePattern '^(?!Integration::)'",
    "test-integration": "jest --testNamePattern ^Integration::",
    "test:cov": "jest --coverage",
    "lint": "f2elint scan",
    "fix": "f2elint fix"
  },
  "dependencies": {
    "@serverless-devs/core": "latest",
    "js-yaml": "^4.0.0",
    "lodash": "^4.17.21",
    "fs-extra": "10.0.0"
  },
  "devDependencies": {
    "@alicloud/fc2": "^2.3.0",
    "@types/fs-extra": "^9.0.12",
    "@types/jest": "^26.0.10",
    "@types/lodash": "^4.14.168",
    "@types/node": "^14.0.23",
    "@types/eslint": "^7.2.6",
    "f2elint": "^0.4.4",
    "dotenv": "^10.0.0",
    "jest": "^26.4.0",
    "ts-jest": "^26.2.0",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.7",
    "esbuild": "^0.13.8"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node"
  }
}
